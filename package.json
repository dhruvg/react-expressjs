{
  "name": "react-expressjs",
  "version": "4.0.0",
  "description": "Boilerplate for my React + Redux + Express projects",
  "main": "server/bin/server.js",
  "scripts": {
    "build:dev:server": "rm -rf ./server/bin && webpack --config webpack/server.dev.js",
    "build:dev:client": "webpack-dev-server --config webpack/client.dev.js",
    "build:dev": "npm-run-all --parallel build:dev:server build:dev:client",
    "build": "rm -rf ./server/bin && npm-run-all --parallel build:client build:server",
    "build:server": "NODE_ENV=production webpack --config webpack/server.prod.js --progress",
    "build:client": "rm -rf ./server/public && NODE_ENV=production webpack --config webpack/client.prod.js --progress",
    "start": "cd server && node bin/server.js",
    "lint": "eslint --ext .jsx,.js ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhruvg/react-expressjs"
  },
  "keywords": [
    "react",
    "express",
    "webpack",
    "babel",
    "expressjs",
    "react router v4",
    "redux"
  ],
  "author": "Dhruv Garg",
  "contributors": [
    "Tahnik Mustasin"
  ],
  "license": "MIT",
  "homepage": "https://github.com/dhruvg/react-expressjs#readme",
  "dependencies": {
    "ejs": "^2.5.7",
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^3.5.3",
    "webpack-dev-server": "^2.4.1",
    "webpack-node-externals": "^1.6.0"
  },
  "bundledDependencies": [
    "ejs",
    "express",
    "prop-types",
    "react",
    "react-dom",
    "react-redux",
    "react-router",
    "react-router-dom",
    "redux"
  ]
}
